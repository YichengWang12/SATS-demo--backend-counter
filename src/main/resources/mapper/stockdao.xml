<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="stockMapper">
    <select id="queryStock" resultType="Map">
        select
            t.code,
            t.name,
            t.abbrName
        from t_stock_us t
    </select>

</mapper>